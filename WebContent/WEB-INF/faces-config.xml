<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	<application>
		<locale-config>
			<default-locale>pt</default-locale>
			<supported-locale>en</supported-locale>
		</locale-config>
		<resource-bundle>
			<base-name>com.cursystem.textos</base-name>
			<var>msg</var>
		</resource-bundle>
	</application>
	<managed-bean>
		<managed-bean-name>grafoBean</managed-bean-name>
		<managed-bean-class>controle.GrafoBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>usuarioBean</managed-bean-name>
		<managed-bean-class>controle.UsuarioBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>autenticadorBean</managed-bean-name>
		<managed-bean-class>controle.AutenticadorBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>especialidadeBean</managed-bean-name>
		<managed-bean-class>controle.EspecialidadeBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pesquisadorBean</managed-bean-name>
		<managed-bean-class>controle.PesquisadorBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>desafioBean</managed-bean-name>
		<managed-bean-class>controle.DesafioBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>reacaoBean</managed-bean-name>
		<managed-bean-class>controle.ReacaoBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>respostaBean</managed-bean-name>
		<managed-bean-class>controle.RespostaBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>analiseRespostaBean</managed-bean-name>
		<managed-bean-class>controle.AnaliseRespostaBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>servicoRespostas</managed-bean-name>
		<managed-bean-class>controle.ServicoRespostas</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>comentarioBean</managed-bean-name>
		<managed-bean-class>controle.ComentarioBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>resolucaoDesafiosBean</managed-bean-name>
		<managed-bean-class>controle.ResolucaoDesafiosBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<validator>
		<display-name>ValidadorEmail</display-name>
		<validator-id>validadorEmail</validator-id>
		<validator-class>util.EmailValidator</validator-class>
	</validator>
</faces-config>
